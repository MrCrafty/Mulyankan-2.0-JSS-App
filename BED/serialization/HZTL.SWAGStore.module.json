{
    "namespace": "HZTL.SWAGStore",
    "items": {
        "includes": [
           {
                "name": "Templates",
                "path": "/sitecore/Templates",
                "database": "master",
                "rules": [      
                    {
                        "path": "/Feature/swag-store",
                        "scope": "ItemAndDescendants",
                        "allowedPushOperations": "CreateUpdateAndDelete"
                    },
					{
                        "path": "/Project",
                        "scope": "ItemAndDescendants",
                        "allowedPushOperations": "CreateUpdateAndDelete"
                    },
					{
                        "path": "*",
                        "scope": "ignored"
                    }
                ]
            },
            {
                "name": "Placeholders",
                "path": "/sitecore/layout/Placeholder Settings",
                "database": "master",
                "rules": [
                    {
                        "path": "/Project",
                        "scope": "ItemAndDescendants",
                        "allowedPushOperations": "CreateUpdateAndDelete"
                    },
                    {
                        "path": "*",
                        "scope": "ignored"
                    }
                ]
            },
			{
                "name": "Media",
                "path": "/sitecore/media library",
                "database": "master",
                "rules": [
                    {
                        "path": "/Project/Horizontal",
                        "scope": "ItemAndDescendants",
                        "allowedPushOperations": "CreateUpdateAndDelete"
                    },
                    {
                        "path": "*",
                        "scope": "ignored"
                    }
                ]
            },
			{
                "name": "Renderings",
                "path": "/sitecore/layout/Renderings",
                "database": "master",
                "rules": [
                    {
                        "path": "/Feature/swag-store",
                        "scope": "ItemAndDescendants",
                        "allowedPushOperations": "CreateUpdateAndDelete"
                    },
                    {
                        "path": "*",
                        "scope": "ignored"
                    }
                ]
            }
            ,
			{
                "name": "Content-home",
                "path": "/sitecore/content/Horizontal/swag-store",
                "database": "master",
                "rules": [
                    {
                        "path": "/Home",
                        "scope": "ItemAndDescendants",
                        "allowedPushOperations": "CreateAndUpdate"
                    },
                    {
                        "path": "*",
                        "scope": "ignored"
                    }
                ]
            }
        ]
    }
}